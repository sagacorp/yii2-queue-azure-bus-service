{
  "name": "sagacorp/yii2-queue-azure-service-bus",
  "description": "Yii2 Queue driver for Azure service bus",
  "license": "MIT",
  "keywords": ["azure", "service", "microsoft", "bus", "yii2-queue", "yii2", "queue", "yii"],
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Adrien Herbert"
    },
    {
      "name": "Aurélien Chrétien"
    }
  ],
  "require": {
    "php": "^8.3",
    "nesbot/carbon": "^2.0 | ^3.0",
    "yiisoft/yii2-httpclient": "^2.0",
    "yiisoft/yii2-queue": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "rector/rector": "^2.1",
    "laravel/pint": "^1.24"
  },
  "autoload": {
    "psr-4": {
      "sagacorp\\queue\\azure\\": "src"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
